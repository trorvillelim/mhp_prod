    "body-parser": "^1.16.1",
    "bunyan": "*",
    "config.json": "*",
    "cookie-parser": "1.4.3",
    "cors": "2.7.1",
    "express": "4.14.1",
    "mime": "1.3.4",
    "moment": "2.16.0",
    "moment-timezone": "0.5.10",
    "ms": "0.7.2",
    "mssql": "*",
    "multer": "1.3.0",
    "nanotimer": "*",
